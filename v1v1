local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "FxZ Hub Beta", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "OPEN SOURCE",
	Callback = function()
      		print("button pressed")
	loadstring(game:HttpGet('https://raw.githubusercontent.com/TheLastqer/tRY23/refs/heads/main/23213'))()
  	end    
})
